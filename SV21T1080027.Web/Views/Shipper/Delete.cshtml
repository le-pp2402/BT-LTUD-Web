@{
    ViewBag.Title = "Xoá đơn vị giao hàng";
}
<div class="box box-danger">
    <div class="box-body">
        <form action="~/Shipper/Save" method="post">
            <div class="form-group">
                <label class="form-label">Tên đơn vị giao hàng: </label>
                <p class="form-control-static"> Giao hàng Tiết kiệm </p>
            </div>

            <div class="form-group">
                <label class="form-label"> Số điện thoại: </label>
                <p class="form-control-static"> 0234.3.882.111 </p>
            </div>

            <div class="form-group text-right">
                <button type="submit" class="btn btn-danger">
                    <i class="fa fa-floppy-o"></i>
                    Xác nhận xoá
                </button>
                <a href="~/Shipper" class="btn btn-default">
                    Quay lại
                </a>
            </div>
        </form>
    </div>
</div>



